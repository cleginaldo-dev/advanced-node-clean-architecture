{
  "name": "advanced_node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config dist/main",
    "dev": "ts-node-dev --respawn --transpile-only --clear -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:unit": "npm run test -- --watch",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:coverage": "npm run test -- --coverage",
    "test:fb-api": "npm run test -- -c ./jest.integration.config.js tests/external/facebook-api.test.ts",
    "eslint:fix": "eslint --fix './src/**' --ext .ts",
    "m:run": "yarn typeorm migration:run",
    "m:revert": "yarn typeorm migration:revert",
    "m:show": "yarn typeorm migration:show",
    "m:create": "yarn typeorm migration:create"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest-mock/express": "^2.0.2",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.4.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.1.0",
    "pg-mem": "1.9.11",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "pg": "8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typeorm": "0.2.29",
    "uuid": "^9.0.0"
  }
}
